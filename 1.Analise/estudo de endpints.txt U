(2) /POST          /filme/

request
{
    "nome": "",
    "sinopse": "", 
    "avaliaçao": 0.0,
    "lançamento":"2020-10-00",
    "disponivel": true,
    "imagem": 0 
}

response 
{
      "id"1,
      "nome":"",
      "sinopse": "",
      "avaliaçao":0.0,
      "lançamento":"2020-10-00",
      "disponivel": true,
      "usuario": 0
}


///////////////////////////////////////////////////////


(3) /PUT   /filme/:id

request body
{
    "nome": "",
    "sinopse": "", 
    "avaliaçao": 0.0,
    "lançamento":"2020-10-00",
    "disponivel": true,
    "imagem": 0,
    "usuario": 0
}

response (204)
-- sem conteudo

///////////////////////////////////////////

(4) /DELETE /filme/:id

response (204)


